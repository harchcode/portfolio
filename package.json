{
  "name": "hartono-chandra-portfolio",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "serve": "vite preview",
    "lint": "eslint src",
    "lint-staged": "lint-staged",
    "prepare": "husky install",
    "size": "size-limit"
  },
  "devDependencies": {
    "@size-limit/file": "^7.0.3",
    "@typescript-eslint/eslint-plugin": "^5.1.0",
    "@typescript-eslint/parser": "^5.1.0",
    "autoprefixer": "^10.4.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.2",
    "lint-staged": "^11.2.3",
    "postcss": "^8.4.1",
    "prettier": "^2.4.1",
    "size-limit": "^7.0.3",
    "tailwindcss": "^2.2.19",
    "typescript": "^4.3.2",
    "vite": "^2.6.4"
  },
  "lint-staged": {
    "src/**/*.{js,ts}": [
      "eslint --fix"
    ],
    "*.{html,css}": [
      "prettier --write"
    ]
  },
  "size-limit": [
    {
      "path": "dist",
      "limit": "10 kB"
    }
  ]
}
